            <div id="hindi_keyboard">

                <table id="number_key" class="bangali">
                    <tbody>
                        <tr>
                            <td class="_key">
                                <span class="_right_char">~</span> <br>
                                <span class="_right_char">`</span>
                            </td>

                            <td class="_key">
                                <span class="_right_char">!</span> <br>
                                <span class="_right_char">1</span> <br>
                                <span class="_right_char">੍ਵ</span>
                            </td>

                            <td class="_key">
                                <span class="_right_char">@@</span> <br>
                                <span class="_right_char">2</span> <br>
